{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "TelegramBotConfiguration": {
    "BotToken": "YOUR_BOT_TOKEN",
    "SecretToken": "SOME-SECRET-STRING",
    "BotWebhookUrl": "https://your.domain.com/bot/update-receiver"
  },
  "HuggingFaceConfiguration": {
    "AuthToken": "YOUR_HF_TOKEN",
    "BaseUrl": "https://api-inference.huggingface.co/models/",
    "ImageGenerator": "stabilityai/stable-diffusion-3.5-large",
    "TextGenerator": "meta-llama/Llama-3.2-3B-Instruct",
    "TextEndpoint": "/v1/chat/completions"
  },
  "SupabaseConfiguration": {
    "AuthToken": "YOUR_SUPABASE_TOKEN",
    "BaseUrl": "https://your.supabase.co"
  }
}
